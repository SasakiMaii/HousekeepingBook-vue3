// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Posts{
  id Int @default(autoincrement()) @id
  item String
  price Int
  income Boolean
  spending Boolean
  comment String
  cretedAt DateTime
}

